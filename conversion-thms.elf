%theorem methtpmatch-implies-proctypematch :
        forall* {CM} {MM} {PM} {MT} {W} {L} {F} {M}
        forall  {CM-L: clsmap-oflist CM L}
                {CM-CM: clsmap-clsmap CM CM}
                {CM-MM: clsmap-methmap CM MM}
                {CM2PM: clsmap2predmap CM PM}
                {MM2W: methmap2progtype PM CM MM W}
                {MM-LP: methmap`lookup MM M MT}
                {MC: methtpmatch CM MM L F MT}
        exists  {PT} {W-LP: progtype`lookup W M PT}
                {PC: proctypematch W F PT}
        true.                


%theorem nulltp-conversion-ok :
	forall* {PM} {CM} {MM} {L} {W} {Pi}
                {K: kind} {E: term K} {Rslt: result K}
	forall  {CM-L: clsmap-oflist CM L}
		{CM-CM: clsmap-clsmap CM CM}
		{CM-MM: clsmap-methmap CM MM}
		{CM2PM: clsmap2predmap CM PM}		 
		{NTYP: nullTyping CM MM L E Rslt}
		{MM2W: methmap2progtype PM CM MM W}
		{L2P: oflist2perm PM L Pi}
	exists  {Out: output K} {TYP: typing W Pi E Out}
		{PO: perm-output PM Pi Rslt Out}
	true.
	
- : nulltp-conversion-ok _ _ _ _ (nulltLit L) _ L2P _ 
     (tTrans2 tObjLoc 
	(transform/trans
	   (transform/implies I)
	   (transform/gen-exists _)))
     (perm-output/expr T2P)
   <- oflist-lookup-implies-clspred _ L L2P _ T2P I.
 
- : nulltp-conversion-ok _ _ _ _ nulltNull _ _ _ 
     (tTrans2 tObjLoc 
	(transform/trans
	   (transform/implies
	      (implies/trans3
		 (implies/equiv (equiv/symmetric equiv/identity))
		 (implies/equiv equiv/commute)
		 (implies/combine
		    (implies/trans
		       (implies/empty2true)
		       (implies/nonlinear bimplies/objequal-reflexive))
		    (implies/reflexive))))
	   (transform/gen-exists null)))
     (perm-output/expr nulltp2perm/null).

- : nulltp-conversion-ok
     CM-L CM-CM CM-MM CM2PM (nulltLet ([o] NTYPF o) NTYPE) MM2W L2P _
     (tTrans2
	(tLet
	   TYP1
	   (letTyping/exists ([v] 
	      letTyping/base v
	  	 (tTrans2 (TYP2 v)
		    (transform/inside ([v]
		       (transform/trans
			  (transform/implies
			     (implies/trans3
				(implies/equiv equiv/associate)
				(implies/combine (implies/equiv equiv/commute)
				   implies/reflexive)
				(implies/equiv 
				   (equiv/symmetric equiv/associate))))
			  (transform/drop))))))))
	(transform/rem-unused))
     (perm-output/expr T2P2)
     <- nulltp-conversion-ok 
	CM-L CM-CM CM-MM CM2PM NTYPE MM2W L2P _ TYP1 (perm-output/expr T2P1)
     <- typing-implies-clsmap-nulltp CM-CM CM-MM CM-L NTYPE CM-T1
     <- ({o} nulltp-conversion-ok (clsmap-oflist/+ CM-T1 CM-L) CM-CM CM-MM CM2PM
	   (NTYPF o) MM2W (oflist2perm/+ T2P1 L2P) _ (TYP2 o) (perm-output/expr T2P2)).
	   

- : nulltp-conversion-ok
     CM-L CM-CM CM-MM CM2PM (nulltIf NTYPE2 NTYPE1 NTYPC) 
     MM2W L2P _ (tTrans2 (tIf TYPC IFTYP) T) EPO
     <- nulltp-conversion-ok
	CM-L CM-CM CM-MM CM2PM NTYPC MM2W L2P _ TYPC CPO
     <- nulltp-conversion-ok
	CM-L CM-CM CM-MM CM2PM NTYPE1 MM2W L2P _ TYP1 EPO
     <- nulltp-conversion-ok
	CM-L CM-CM CM-MM CM2PM NTYPE2 MM2W L2P _ TYP2 EPO
     <- conversion-helper/if TYP1 TYP2 EPO CPO _ IFTYP T.

- : nulltp-conversion-ok
     CM-L CM-CM CM-MM CM2PM
     (nulltIfNotNull NTYP1 NTYP2 (OF-LP:oflist-lookup _ O _)) MM2W L2P _
     (tIf 
	(tTrans2 (tNot tEql notEnv/base)
	   (transform/cond
	      (bimplies/reflexive)
	      (bimplies/reflexive)
	      (implies/trans3
		 (implies/combine implies/reflexive I1)
		 (implies/equiv equiv/associate)
		 (implies/combine
		    (I2 O)
		    (implies/reflexive)))
	      (implies/trans3
		 (implies/combine
		    (implies/trans
		       (implies/nonlinear bimplies/tt)
		       (implies/true2empty))
		    (implies/reflexive))
		 (implies/equiv equiv/commute)
		 (implies/equiv equiv/identity))))
	(ifTyping/base 
	   (tTrans2 TYP1
	      (transform/inside [_]
		 (transform/implies
		    (implies/combine
		       (implies/reflexive)
		       (implies/trans3
			  (implies/combine
			     (implies/trans
				(implies/nonlinear bimplies/tt)
				(implies/true2empty))
			     (implies/reflexive))
			  (implies/equiv equiv/commute)
			  (implies/equiv equiv/identity))))))
	   TYP2)) EPO
     <- oflist-lookup-implies-clsmap-nulltp CM-L OF-LP (clsmap-nulltp/norm IN)
     <- oflist-lookup-implies-clspred _ OF-LP L2P _ (nulltp2perm/norm PM-LP NN2P) I1
     <- clspred-remove-cond PM-LP NN2P ([o] I2 o)
     <- nulltp-conversion-ok CM-L CM-CM CM-MM CM2PM NTYP2 MM2W L2P _ TYP2 EPO
     <- nulltp-conversion-ok (clsmap-oflist/+ (clsmap-nulltp/norm IN) CM-L)
	CM-CM CM-MM CM2PM NTYP1 MM2W 
	(oflist2perm/+ (nulltp2perm/norm PM-LP nn2perm/yes) L2P) _ TYP1 _.

- : nulltp-conversion-ok CM-L CM-CM CM-MM CM2PM (nulltWhile NTYPE NTYPC) MM2W L2P _
     (tTrans2 (tWhile (transform/reflexive) (letTyping/base null TYPC) WT DV T1) 
	(transform/trans3 T2
	   (transform/implies
	      (implies/trans3
		 (implies/equiv (equiv/symmetric equiv/identity))
		 (implies/equiv equiv/commute)
		 (implies/combine
		    (implies/trans
		       (implies/empty2true)
		       (implies/nonlinear bimplies/objequal-reflexive))
		    (implies/reflexive))))
	   (transform/gen-exists null)))
     (perm-output/expr nulltp2perm/null)
     <- nulltp-conversion-ok CM-L CM-CM CM-MM CM2PM NTYPE MM2W L2P _ TYPE (perm-output/expr _)
     <- nulltp-conversion-ok CM-L CM-CM CM-MM CM2PM NTYPC MM2W L2P _ TYPC CPO
     <- conversion-helper/while (tTrans2 TYPE (transform/inside ([v] transform/drop))) 
	CPO _ _ _ WT DV T1 T2.
	
- : nulltp-conversion-ok CM-L CM-CM CM-MM CM2PM nulltArgs/0 MM2W L2P 
