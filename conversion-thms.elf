- : nulltp-conversion-ok
     CM-L CM-CM CM-MM CM2PM (nulltArgs/+ ([o] NTYPA o) NTYP) MM2W L2P _ 
     (tArgs1 (TYP)
	(letTyping/exists [v]
	   (letTyping/base v (tArgs (TYPA v))))) 
     (perm-output/args/+ APO T2P)
     <- nulltp-conversion-ok 
